{
  "name": "sprint6dicegame",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/models/index.js",
    "dev": "nodemon --exec ts-node-dev src/server.ts",
    "build": "tsc",
    "prestart": "npm run build",
    "test": "jest --watch --runInBand --detectOpenHandles"  },
  "keywords": [],
  "engines": {},
  "author": "Glòria Monzó",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aredhel269/Sprint6DiceGame.git"
  },
  "homepage": "https://github.com/Aredhel269/Sprint6DiceGame#readme",
  "bugs": {
    "url": "https://github.com/Aredhel269/Sprint6DiceGame/issues"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "4.19.1",
    "jsonwebtoken": "9.0.2",
    "mysql2": "3.9.3",
    "sequelize": "6.37.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "jest": "29.7.0",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.5.4"
  },
  "nodemon": {
    "exec": "ts-node src/server.ts",
    "watch": [
      "src"
    ],
    "ext": "ts"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "./node_modules/",
      "./dist/"
    ]
  }
}
